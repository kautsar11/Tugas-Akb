<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tugasakb"
    >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SensorMaps10120155"
        tools:targetApi="31">

        <activity
            android:name="com.example.tugasakb.SplashScreen"
            android:exported="true"
            android:label="Notes AKB">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.tugasakb.ui.navigation.NavigationActivity"
            android:exported="false"/>

        <activity
            android:name="com.example.tugasakb.ui.navigation.GetStarted"
            android:exported="false">

        </activity>

        <activity
            android:name="com.example.tugasakb.MainActivity"
            android:exported="false">

        </activity>

    </application>

</manifest>